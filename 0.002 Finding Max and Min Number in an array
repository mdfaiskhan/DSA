public class Main{
    public static void main(String[] args){
        System.out.println("Hello");
        int array[]={99,12,56,-45,86};
        int max=array[0];
        int min=array[0];
        
        for(int i=0;i<array.length;i++){
            if(array[i]>max){
                max=array[i];
            }
            if(array[i]<min){
                min=array[i];
            }
        }
        System.out.println("Maximum Number"+max);
            System.out.println("min number"+min);
    }
}
